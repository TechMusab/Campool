{
    "version": 2,
    "builds": [
        {
            "src": "src/index-simple.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/src/index-simple.js"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}